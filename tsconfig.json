{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#api/*": ["./src/api/*"],
      "#assets/*": ["./src/assets/*"],
      "#components/*": ["./src/components/*"],
      "#hooks/*": ["./src/hooks/*"],
      "#mocks/*": ["./src/mocks/*"],
      "#navigation/*": ["./src/navigation/*"],
      "#services/*": ["./src/services/*"],
      "#tsyringe/*": ["./src/tsyringe/*"],
      "#utils/*": ["./src/utils/*"]
    },
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
